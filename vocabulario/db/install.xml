  <TABLES>
     <TABLE NAME="vocabulario" COMMENT="Default comment for vocabulario, please edit me">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="Course vocabulario activity belongs to" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="name field for moodle instances" PREVIOUS="course" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" COMMENT="General introduction of the vocabulario activity" PREVIOUS="name" NEXT="introformat"/>
        <FIELD NAME="introformat" TYPE="int" LENGTH="4" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="Format of the intro field (MOODLE, HTML, MARKDOWN...)" PREVIOUS="intro" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="introformat" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="timecreated"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="course" UNIQUE="false" FIELDS="course"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="vocabulario_sustantivos" COMMENT="sustantivo" PREVIOUS="vocabulario" NEXT="vocabulario_adjetivos">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"  NEXT="palabra"/>
        <FIELD NAME="palabra" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"  COMMENT="palabra en sÃ­ misma" PREVIOUS="id" NEXT="genero"/>
        <FIELD NAME="genero" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"  COMMENT="genero del nombre 0 = neutro 1 = masculino 2 = femenino" PREVIOUS="palabra" NEXT="plural"/>
        <FIELD NAME="plural" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false"  COMMENT="plural del nombre" PREVIOUS="genero" NEXT="significado"/>
        <FIELD NAME="significado" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false"  COMMENT="traduccion de la palabra" PREVIOUS="plural" NEXT="observaciones"/>
        <FIELD NAME="observaciones" TYPE="text" LENGTH="medium" NOTNULL="false" SEQUENCE="false"  COMMENT="observaciones del sustantivo" PREVIOUS="significado" NEXT="gramaticaid"/>
        <FIELD NAME="gramaticaid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"  COMMENT="id de la gramatica" PREVIOUS="observaciones" NEXT="intencionid"/>
        <FIELD NAME="intencionid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"  COMMENT="id de la intencion" PREVIOUS="gramaticaid" NEXT="tipologiaid"/>
        <FIELD NAME="tipologiaid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false"  COMMENT="id de la tipologia" PREVIOUS="intencionid" NEXT="ejemplo"/>
        <FIELD NAME="ejemplo" TYPE="char" LENGTH="100" NOTNULL="false" SEQUENCE="false"  COMMENT="ejemplo" PREVIOUS="tipologiaid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="gram_id"/>
        <KEY NAME="gram_id" TYPE="foreign" FIELDS="gramaticaid" REFTABLE="vocabulario_gramatica" REFFIELDS="id" COMMENT="id de la gramatica" PREVIOUS="primary" NEXT="inten_id"/>
        <KEY NAME="inten_id" TYPE="foreign" FIELDS="intencionid" REFTABLE="vocabulario_intenciones_es" REFFIELDS="id" COMMENT="id de la intencin" PREVIOUS="gram_id" NEXT="tipo_id"/>
        <KEY NAME="tipo_id" TYPE="foreign" FIELDS="tipologiaid" REFTABLE="vocabulario_tipologias_es" REFFIELDS="id" COMMENT="id de la tipologia" PREVIOUS="inten_id"/>
      </KEYS>
    </TABLE>
