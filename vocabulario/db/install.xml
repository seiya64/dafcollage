      </KEYS>
    </TABLE>
    <TABLE NAME="vocabulario_gramatica" COMMENT="estructuras gramaticales" PREVIOUS="vocabulario_camposlexicos_fr" NEXT="vocabulario_mis_palabras">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"  NEXT="usuarioid"/>
        <FIELD NAME="usuarioid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"  COMMENT="id del usuario" PREVIOUS="id" NEXT="padre"/>
        <FIELD NAME="padre" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"  COMMENT="padre en la jerarquía de gramáticas" PREVIOUS="usuarioid" NEXT="gramatica"/>
        <FIELD NAME="gramatica" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false"  COMMENT="nombre para la estructura gramatical" PREVIOUS="padre"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="clave_usuario"/>
        <KEY NAME="clave_usuario" TYPE="foreign" FIELDS="usuarioid" REFTABLE="mdl_user" REFFIELDS="id" COMMENT="id del usuario" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
    <TABLE NAME="vocabulario_mis_palabras" COMMENT="palabras que guarda cada usuario" PREVIOUS="vocabulario_gramatica" NEXT="vocabulario_mis_gramaticas">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true"  NEXT="usuarioid"/>
        <FIELD NAME="usuarioid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"  COMMENT="id del usuario al que pertenece la palabra" PREVIOUS="id" NEXT="sustantivoid"/>
        <FIELD NAME="sustantivoid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false"  COMMENT="id del sustantivo que guarda cada usuario" PREVIOUS="usuarioid" NEXT="adjetivoid"/>
        <FIELD NAME="adjetivoid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"  PREVIOUS="sustantivoid" NEXT="verboid"/>
        <FIELD NAME="verboid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"  PREVIOUS="adjetivoid" NEXT="otroid"/>
        <FIELD NAME="otroid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"  PREVIOUS="verboid" NEXT="campoid"/>
        <FIELD NAME="campoid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="1" SEQUENCE="false"  PREVIOUS="otroid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="clave_usuario"/>
        <KEY NAME="clave_usuario" TYPE="foreign" FIELDS="usuarioid" REFTABLE="mdl_user" REFFIELDS="id" COMMENT="id del usuario" PREVIOUS="primary" NEXT="clave_sustantivo"/>
        <KEY NAME="clave_sustantivo" TYPE="foreign" FIELDS="sustantivoid" REFTABLE="vocabulario_camposlexicos_es" REFFIELDS="id" COMMENT="id del sustantivo" PREVIOUS="clave_usuario" NEXT="clave_campo"/>
        <KEY NAME="clave_campo" TYPE="foreign" FIELDS="campoid" REFTABLE="vocabulario_camposlexicos_es" REFFIELDS="id" COMMENT="id del campo lexico" PREVIOUS="clave_sustantivo" NEXT="clave_adjetivo"/>
        <KEY NAME="clave_adjetivo" TYPE="foreign" FIELDS="adjetivoid" REFTABLE="vocabulario_adjetivos" REFFIELDS="id" COMMENT="id del adjetivo" PREVIOUS="clave_campo" NEXT="clave_verbo"/>
        <KEY NAME="clave_verbo" TYPE="foreign" FIELDS="verboid" REFTABLE="vocabulario_verbos" REFFIELDS="id" COMMENT="id del verbo" PREVIOUS="clave_adjetivo" NEXT="clave_otro"/>
        <KEY NAME="clave_otro" TYPE="foreign" FIELDS="otroid" REFTABLE="vocabulario_otros" REFFIELDS="id" COMMENT="id de la otra palabra" PREVIOUS="clave_verbo" NEXT="clave_norepetir"/>
        <KEY NAME="clave_norepetir" TYPE="unique" FIELDS="sustantivoid, adjetivoid, verboid, otroid, campoid" COMMENT="clave para no repetir tuplas" PREVIOUS="clave_otro"/>
